sudo rm -rf /var/www/html/drupal
sudo mkdir -p /var/www/html/drupal
wget https://ftp.drupal.org/files/projects/drupal-9.5.9.tar.gz
tar xvf drupal-9.5.9.tar.gz
sudo mv drupal-9.5.9/* /var/www/html/drupal/
sudo mv drupal-9.5.9/.htaccess /var/www/html/drupal/
sudo chown -R www-data:www-data /var/www/html/drupal
sudo chmod -R 755 /var/www/html/drupal
